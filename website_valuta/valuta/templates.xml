<openerp>
    <data>
        <template id="index">
            <t t-call="website.layout">
                <t t-set="title">Currencies</t>
                <div class="oe_structure">
                    <div class="container">
                        <t t-foreach="currencies" t-as="currency">
                            <p><t t-esc="currency.id"/> <t t-esc="currency.name"/></p>
                        </t>
                    </div>
                </div>

        </template>
    </data>
</openerp>